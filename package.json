{
  "name": "sljs",
  "version": "0.0.0",
  "description": "A framework for connecting to Linden Lab's virtual world \"Second Life\"",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build",
    "start": "babel-node --harmony-async-await ./src/index.js",
    "test": "npm run lint",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      "transform-es2015-modules-commonjs",
      "transform-export-extensions"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwigz/sljs.git"
  },
  "keywords": [
    "api",
    "bot",
    "framework",
    "library",
    "life",
    "node",
    "second",
    "secondlife",
    "sl",
    "sljs"
  ],
  "author": "gwigz <git.sljs@gwi.gr>",
  "contributors": {
    "name": "Gwigz"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/gwigz/sljs/issues"
  },
  "homepage": "https://github.com/gwigz/sljs#readme",
  "engines": {
    "node": ">=7.5.0"
  },
  "dependencies": {
    "long": "^3.2.0",
    "xmlrpc": "^1.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-plugin-import": "^2.0.1",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-strip-async-await": "^0.1.0",
    "minami": "^1.1.1"
  }
}
